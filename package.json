{
  "name": "microsoft-graph-toolkit",
  "version": "0.0.1",
  "description": "Microsoft Graph Toolkit",
  "main": "dist/es6/bundle/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "start": "run-p build:watch serve watch",
    "build": "npm run clean && npm run build:rollup && npm run build:sass && npm run build:ts-raw",
    "build:sass": "gulp sass",
    "build:sass:watch": "gulp sass:watch",
    "build:ts-raw-es5": "tsc -p es5.tsconfig.json",
    "build:ts-raw-es6": "tsc -p es6.tsconfig.json",
    "build:ts-raw": "npm run build:ts-raw-es6 && npm run build:ts-raw-es5",
    "build:rollup": "rollup -c rollup.config.js",
    "build:watch": "rollup -w -c rollup.config.js",
    "serve": "owc-dev-server --port 3333",
    "watch": "browser-sync start --no-notify --proxy localhost:3333 --files 'dist/**/*.js, index.html'",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "@metulev",
  "license": "MIT",
  "dependencies": {
    "@microsoft/teams-js": "^1.4.1",
    "lit-element": "^2.1.0",
    "msal": "^0.2.4",
    "office-ui-fabric-core": "^9.6.1"
  },
  "devDependencies": {
    "@microsoft/microsoft-graph-types": "^1.7.0",
    "@types/jest": "^24.0.11",
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "browser-sync": "^2.26.3",
    "node-sass": "^4.11.0",
    "jest": "^24.5.0",
    "jest-junit": "^6.3.0",
    "npm-run-all": "^4.1.5",
    "owc-dev-server": "^0.2.1",
    "rollup": "^1.7.3",
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript": "^1.0.1",
    "sass": "^1.17.3",
    "shx": "^0.3.2",
    "ts-jest": "^24.0.0",
    "tslib": "^1.9.3",
    "typescript": "^3.3.4000",
    "gulp": "^4.0.0",
    "gulp-append-prepend": "^1.0.6",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "testResults/junit.xml",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  }
}
